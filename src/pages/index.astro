---
import { Image } from 'astro:assets';
import avatar from '../assets/avatar.jpeg';
import Base from '../layouts/Base.astro';

---
<Base title="aslushnikov.com">
  <style>
    :root {
      color: blue;
      --image-size: 250px;
      --icon-size: 40px;
      --icon-padding: 8px;
    }

    @keyframes drop {
        0% { transform: scale(0); }
        100% { transform: scale(1);  }
    }
  
    * {
      box-sizing: border-box;
    }

    .icon {
      display: flex;
      align-items: center;
      justify-content: center;
      width: var(--icon-size);
      padding: var(--icon-padding);
      flex: none;
      
      animation-duration: 0.5s;
      animation-timing-function: ease;
      animation-name: drop;

      transform: scale(0);
      animation-fill-mode: forwards;

      svg {
        fill: white;
      }
    }
  
    .gmail { animation-delay: 0.0s }
    .medium { animation-delay: 0.05s }
    .twitter { animation-delay: 0.1s }
    .facebook { animation-delay: 0.15s }
    .github { animation-delay: 0.2s }
    .instagram { animation-delay: 0.25s }
    .linkedin { animation-delay: 0.20s }

    .social:hover {
      box-shadow: 1px 1px 4px 0px #555;
    }

    .hobby {
      svg {
        fill: #999;
        transition: all 0.2s;
      }
    }

    .icon.hobby:hover svg {
      fill: black;
      transform: scale(1.2);
    }
  
    img {
      outline: 2px solid white;
      box-shadow: 0 0 4px #424242;
      width: var(--image-size);
      height: var(--image-size);
    }

    section {
      margin: 0 auto;
      width: fit-content;
      margin-top: 10vh;

      display: grid;
      grid-template-columns: repeat(1, var(--icon-size)) auto repeat(4, var(--icon-size));
      grid-template-rows: repeat(2, var(--icon-size)) auto repeat(4, var(--icon-size));
      gap: 15px;

      .backpacking {
        grid-row: 1/2;
        grid-column: 1/2;
        padding: 0;
      }

      .music {
        grid-row: 2/3;
        grid-column: 1/2;
        padding: 0;
      }

      .travelling {
        grid-row: 3/4;
        grid-column: 1/2;
        padding: 0;
      }

      /* middle */
      img {
        grid-column: 2/-2;
        grid-row: 2/-2;
        height: auto;
      }

      /* bottom-right corner */
      .github {
        grid-row: -1/-2;
        grid-column: -4/-5;
      }

      .twitter {
        grid-row: -1/-2;
        grid-column: -3/-4;
      }
      .gmail {
        grid-row: -1/-2;
        grid-column: -2/-3;
      }
      .instagram {
        grid-row: -1/-2;
        grid-column: -1/-2;
      }
      .facebook {
        grid-row: -2/-3;
        grid-column: -1/-2;
      }
      .medium {
        grid-row: -3/-4;
        grid-column: -1/-2;
      }
      .linkedin {
        grid-row: -4/-5;
        grid-column: -1/-2;
      }
    }
  </style>
